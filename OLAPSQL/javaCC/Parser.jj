//pour l'instant, retourne une string
String tdate ():{}
{
	{String dat=""; Token t;}
	t=<ENTIER>{dat= t.toString();}
	<SLASH>t=<ENTIER>{dat=dat+"-"+ t.toString();}
	<SLASH>t=<ENTIER>{dat=dat+"-"+ t.toString();}
	{return dat;}
}


