//tdate renvoit une string, le prend comme token
String def_une_valeur():{}
{
		{Token t=null;String s="";}
		
		(<QUOT> (	s=tdate() 
				| 	(		(t=<NOM> {s = s+t.toString();})
						(t=<NOM> {s = s+" "+t.toString();})*
					)
				)								
		<QUOT>	{return s;})
		
	|	(t=<CHIFFRE>{return t.toString();})		
	|	(t=<ENTIER>{return t.toString();})
}

