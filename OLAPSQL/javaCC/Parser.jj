SKIP:
{" " | "\r" | "\n" | "\t"}


TOKEN:
{
		<CREATE		: "create">
   |	<DROP		: "drop">
   |	<ADD		: "add">
   |	<ALTER		: "alter">
   |	<INSERT		: "insert into">
   |	<DELETE		: "delete from">
   |	<VALUES		: "values">
   |	<SELECT		: "select">
 
   |	<FACT		: "fact">
   |	<LEVEL		: "level">
   |	<WITH		: "with">
   |	<HIERARCHY	: "hierarchy">
   |	<DIMENSION	: "dimension">
   |	<ROW		: "row">
   |	<COLUMN		: "column">
   
   |	<CONNECT	: "connect">
   |	<DISCONNECT	: "disconnect">
   |	<TO			: "to">
   |	<WHERE		: "where">
   |	<FROM		: "from">
   |	<AND		: "and">
   |	<OR			: "or">

//   |    <ENTREQUOT	: >
//symboles
    |	<OUVRANTE	: "(">
    |	<FERMANTE	: ")">
    |	<PTVIRG		: ";">
    |	<PT			: ".">
    |	<VIRG		: ",">
    |	<QUOT		: "'">
    |	<SLASH		: "/">       
//predicats	
/*	|	<EGAL		: "=">
	|	<NEGAL		: "<>">
	|	<SUP		: ">">
	|	<SUPEGAL	: ">=">
	|	<INF		: "<">
	|	<INFEGAL	: "<=">*/
	|	<OPERATOR	: ("="|"<>"|">"|">="|"<"|"<=")>
	|	<FONCT		: ("avg"|"count"|"sum"|"max"|"min")>
// types
   |	<DATE		: "date">
   |	<VARCHAR	: "varchar">
   |	<NUMBER		: "number">
   //doit commencer par une lettre
   |    <ENTIER		: (["0"-"9"])+>
   |    <CHIFFRE	: (["0"-"9"])+ 	((",")(["0"-"9"])+)?>				//(["0"-"9"]|",")*>
   |	<NOM		: (["A"-"Z"])+(["A"-"Z"]|["0"-"9"]|"_"|".")*>
}

